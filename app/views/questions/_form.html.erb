<%= form_for :question, url: questions_path do |f| %>
	<p>
		<%= f.label :title %><br>
		<%= f.text_field :title %>
	</p>
	<p>
		<%= f.label :description %><br>
		<%= f.text_area :description %> 
	</p>
	<p>
		<%= f.label :status %><br>
		<%= f.text_area :status %>
	</p>
	<p>
		 <%= f.label :tag_names %><br>
  	<%= f.text_field :tag_names %>
	</p>
	<p>
		<%= f.submit %>
	</p>
<% end %>